{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2023",
    "checkJs": true,
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*"],
    }
  },
  "include": [
    "src/**/*",
    "app.ts",
    "env.config.js",
    "src/*",
  ],
  "exclude": [
    "node_modules"
  ]
}