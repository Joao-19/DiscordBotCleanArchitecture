{
  "compilerOptions": {
    "module": "ES2022",
    "target": "ES2022",
    "checkJs": true,
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": [
    "src/**/*",
    "app.js",
    "env.config.js"
  ],
  "exclude": [
    "node_modules"
  ]
}